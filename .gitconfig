[alias]
    b = branch
    ci = commit
    co = checkout
    d = diff
    l = log
    ls = log --decorate=short --oneline
    s = status

[user]
    name = Min-Young Wu
    email = mwu368@gmail.com

[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan

[core]
    excludesfile = ~/.gitignore

[branch "master"]
    remote = origin
    merge = refs/heads/master

